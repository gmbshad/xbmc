[Unit]
Description=Samba AD Daemon
Documentation=man:samba(8) man:samba(7) man:smb.conf(5)
Wants=network-online.target
After=network.target network-online.target

[Service]
Type=notify
PIDFile=/Users/Shared/xbmc-depends/macosx13.0_arm64-target-debug/var/run/samba/samba.pid
LimitNOFILE=16384
EnvironmentFile=-/Users/Shared/xbmc-depends/macosx13.0_arm64-target-debug/etc/sysconfig/samba
ExecStart=/Users/Shared/xbmc-depends/macosx13.0_arm64-target-debug/sbin/samba --foreground --no-process-group $SAMBAOPTIONS
ExecReload=/bin/kill -HUP $MAINPID


[Install]
WantedBy=multi-user.target
