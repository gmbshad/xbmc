# Variable needed by LTLIBINTL.
top_builddir=../..

OBJEXT="o"
EXEEXT=""
CC=" /Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/clang"
CFLAGS="-fheinous-gnu-extensions -ftree-vectorize -pipe -Wno-trigraphs -fpascal-strings -Wreturn-type -Wunused-variable -fmessage-length=0 -gdwarf-2 -g -D_DEBUG  -arch arm64 -miphoneos-version-min=11.0 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS16.1.sdk -isystem /Users/Shared/xbmc-depends/iphoneos16.1_arm64-target-debug/include "
CXX=" /Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/clang++"
CXXFLAGS="-stdlib=libc++ -g -D_DEBUG  -std=c++17 -arch arm64 -miphoneos-version-min=11.0 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS16.1.sdk -isystem /Users/Shared/xbmc-depends/iphoneos16.1_arm64-target-debug/include "
CPPFLAGS="-fheinous-gnu-extensions -ftree-vectorize -pipe -Wno-trigraphs -fpascal-strings -Wreturn-type -Wunused-variable -fmessage-length=0 -gdwarf-2 -g -D_DEBUG  -arch arm64 -miphoneos-version-min=11.0 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS16.1.sdk -isystem /Users/Shared/xbmc-depends/iphoneos16.1_arm64-target-debug/include "
LDFLAGS="-L/Users/Shared/xbmc-depends/iphoneos16.1_arm64-target-debug/lib -Wl,-search_paths_first -L/Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS16.1.sdk/usr/lib -arch arm64 -miphoneos-version-min=11.0 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS16.1.sdk -stdlib=libc++  -liconv  "
LTLIBINTL="${top_builddir}/intl/libintl.la   -Wl,-framework -Wl,CoreFoundation"
JAVA_CHOICE="no"
BUILDJAVA="no"
TESTJAVA="no"
CSHARP_CHOICE="no"
BUILDCSHARP="no"
