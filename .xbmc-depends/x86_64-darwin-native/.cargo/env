#!/bin/sh
# rustup shell setup
# affix colons on either side of $PATH to simplify matching
case ":${PATH}:" in
    *:"/Users/redd/kodi/.xbmc-depends/x86_64-darwin-native/.cargo/bin":*)
        ;;
    *)
        # Prepending path in case a system-installed rustc needs to be overridden
        export PATH="/Users/redd/kodi/.xbmc-depends/x86_64-darwin-native/.cargo/bin:$PATH"
        ;;
esac
